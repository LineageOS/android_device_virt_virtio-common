/dev/block/vdc /data ext4 nodev,noatime,nosuid latemount,wait,check,quota,formattable,checkpoint=block
/dev/block/vdc /data f2fs nodev,noatime,nosuid,reserve_root=32768 latemount,wait,check,quota,formattable,fscompress,checkpoint=fs

