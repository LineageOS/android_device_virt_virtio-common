# Block devices
/dev/block/sda		u:object_r:system_block_device:s0
/dev/block/vda2		u:object_r:super_block_device:s0
/dev/block/vda3		u:object_r:misc_block_device:s0
/dev/block/vda4		u:object_r:metadata_block_device:s0
/dev/block/vda5		u:object_r:cache_block_device:s0
/dev/block/vdb		u:object_r:userdata_block_device:s0
/dev/block/vdc		u:object_r:system_block_device:s0

# Console
/dev/hvc[0-9]+		u:object_r:console_device:s0

# Executables
/vendor/bin/dlkm_loader							u:object_r:dlkm_loader_exec:s0
/vendor/bin/setup_wifi							u:object_r:setup_wifi_exec:s0
/vendor/bin/suspend_blocker						u:object_r:suspend_blocker_exec:s0
/vendor/bin/tablet2multitouch						u:object_r:tablet2multitouch_exec:s0
/vendor/bin/wakeupd							u:object_r:wakeupd_exec:s0
/vendor/bin/hw/android\.hardware\.gatekeeper@1\.0-service.software	u:object_r:hal_gatekeeper_default_exec:s0

# Firmware
/vendor/firmware_mnt(/.*)?		u:object_r:firmware_file:s0

# Same Process HAL
/vendor/lib(64)?/hw/gralloc\.minigbm\.so		u:object_r:same_process_hal_file:s0
